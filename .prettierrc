{
    "trailingComma": "none",
    "jsxBracketSameLine": true,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120,
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "overrides": [{
      "files": ["*.json"],
      "options": {
        "tabWidth": 2,
        "singleQuote": false
      }
    }, {
      "files": ["*.html"],
      "options": {
        "tabWidth": 2
      }
    }]
  }
  